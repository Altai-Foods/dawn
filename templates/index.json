    "1634751717114493c3": {
      "type": "multicolumn",
      "blocks": {
        "1634751717114493c3-1": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/2020_1203_TheHappyHowl_LambDelight_FrontRendering_Small_1.png",
            "title": "LAMB DELIGHT",
            "text": "<p>FORMULATED FOR THE \"WE'VE TRIED EVERYTHING\"<\/p><p> - - - - - - - - - - - - - - <\/p><p>grass-fed lamb cooked to perfection & mixed with rice, veggies & berries making this the healthiest recipe known to man[s best friend].<\/p><p>- - - - - - - - - - - - - <\/p><p>BEST FOR:<\/p><p>- hypoallergenic<\/p><p>- pancreatitis <\/p><p>- environmental allergies<\/p><p>- food sensitivities & allergies<\/p><p>- gastrointestinal illnesses<\/p><p>- diarrhea & vomiting<\/p><p>- skin & fur conditions<\/p><p>- all size breeds (best for small)<\/p><p>- picky eaters<\/p>"
          }
        },
        "1634751717114493c3-0": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/2020_1203_TheHappyHowl_TastyTurkeyFeast_FrontRendering_Small_d5a802b9-9225-4670-be45-be16f735845b.png",
            "title": "TASTY TURKEY FEAST",
            "text": "<p><strong>BEST SELLING RECIPE<\/strong><\/p><p>- - - - - - - - - - - - - - -<\/p><p>high-quality lean turkey mixed with the best thanksgiving day foods that your pup is always begging for.<\/p><p>- - - - - - - - - - - - - - -<\/p><p>BEST FOR:<\/p><p>- overweight<\/p><p>- seniors<\/p><p>- less active<\/p><p>- heart disease<\/p><p>- chicken or beef allergy<\/p><p>- all size breeds (best for medium)<\/p><p>- picky eaters<\/p>"
          }
        },
        "1634751717114493c3-2": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/2020_1203_TheHappyHowl_GrandmasBeefStew_FrontRendering_Small_1.png",
            "title": "GRANDMA'S BEEF STEW",
            "text": "<p>THE PACK FAVORITE<\/p><p>- - - - - - - - - - - - - -<\/p><p>high-quality tender beef mixed with freshly cooked fruits and veggies that is everyone's favorite winter crock-pot comfort food. <\/p><p>- - - - - - - - - - - - - -<\/p><p>BEST FOR:<\/p><p>- puppies & adults<\/p><p>- weight maintenance <\/p><p>- diabetic treatment<\/p><p>- sarcoma & tumor preventative<\/p><p>- active dogs<\/p><p>- large breed dogs (best for large)<\/p><p>- picky eaters<\/p><p><\/p>"
          }
        }
      },
