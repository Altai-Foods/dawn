    "1629840873714f362b": {
      "type": "rich-text",
      "blocks": {
        "1629840873714f362b-0": {
          "type": "heading",
          "settings": {
            "heading": "Our meals make us a dog food company. Our hearts make us so much more.",
            "heading_size": "h1"
          }
        },
        "1629840873714f362b-1": {
          "type": "text",
          "settings": {
            "text": "<p>At Happy Howl, We’re dedicated to changing dogs' lives – one bowl at a time, one rescue pup at a time. We’re the first dog food company founded with a mission to change the lives of all dogs alike through good, healthy food that actually gives back.<\/p>"
          }
        }
      },
      "block_order": [
        "1629840873714f362b-0",
        "1629840873714f362b-1"
      ],
      "settings": {
        "color_scheme": "background-2",
